        image.addEventListener("click", function() {
            image.style.display = "none"; // 이미지를 숨깁니다.
            textArea.style.display = "block"; // 텍스트를 표시합니다.
        });

        textArea.addEventListener("click", function() {
            textArea.style.display = "none"; // 텍스트를 숨깁니다.
            image.style.display = "block"; // 이미지를 표시합니다.
        });
    </script>
    
    <script>
    // 각 행의 카드 높이를 맞추는 함수
    function adjustCardHeights() {
        // 각 행을 선택
        var rows = document.querySelectorAll(".card-row");
        rows.forEach(function (row) {
            var cards = row.querySelectorAll(".card");
            var maxHeight = 0;
            // 각 행의 카드 중에서 가장 큰 높이를 찾음
            cards.forEach(function (card) {
                var cardHeight = card.offsetHeight;
                if (cardHeight > maxHeight) {
                    maxHeight = cardHeight;
                }
            });
            // 모든 카드에 동일한 높이를 적용
            cards.forEach(function (card) {
                card.style.height = maxHeight + "px";
            });
        });
    }

    // 페이지 로드 시 카드 높이 조정 함수 실행
    window.addEventListener("load", adjustCardHeights);

    // 창 크기가 변경될 때마다 카드 높이 조정 함수 실행
    window.addEventListener("resize", adjustCardHeights);
</script>
</body>
</html>